            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textview_resultfragment_ageresult"
            android:layout_below="@id/textview_resultfragment_age"
            pt:imageSrc="@drawable/pointblue"
            pt:textSize="16sp"
            pt:marginCenter="50dp"
