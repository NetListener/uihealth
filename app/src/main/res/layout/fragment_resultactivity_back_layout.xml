            pt:textSize="16sp"
            pt:marginCenter="50dp"
            pt:text = "男"
            pt:textColor = "@color/white"
            android:layout_marginLeft="20dp"
            />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textview_resultfragment_diabetes_mellitus"
